.subckt mim_cap  n1 n2
.param
+ l=10u w=10u nc=1 nr=1
+ count='nc*nr' 
+ misfactor=0 scaler='scale_mim'
+ area = 'l*w*count*scaler*scaler'
+ pj   = '2*(l+w)*count*scaler'
+ geo_fac = '1/sqrt(area*1e12)'
+ mis_fac = '0.0000e-3*geo_fac*sigma_mis_mim*misfactor'
+ c_area  = ' 8.2790e-04 * (1+dmim_c0) '
+ c_f     = ' 4.7210e-10 * (1+dmim_c0) '
+ c_tc1=17.70e-6
+ c_tc2=0.001e-8
+ c_vc1=-27.337e-6
+ c_vc2=-1.7886e-6
